<?xml version="1.0" encoding="utf-8" ?>
<RenderGroupResource>
	<!-- Generic classes -->
	<RenderGroup id="test" >
		<property enablelighting="false" />
		<property enabletexture="true" />
		<property enableshadow="false" />
		<property enablefog="true" />
		<property skinned="false" />
	</RenderGroup>
	
	<RenderGroup id="default" >
		<property enablelighting="true" />
		<property enabletexture="true" />
		<property enableshadow="false" />
		<property enablefog="true" />
		<property skinned="false" />
	</RenderGroup>
	
	<RenderGroup id="shadow" >
		<property enablelighting="true" />
		<property enabletexture="true" />
		<property enableshadow="true" />
		<property enablefog="true" />
		<property skinned="false" />
	</RenderGroup>
	
	<RenderGroup id="terrain-shadowed" >
		<property enablelighting="true" />
		<property enabletexture="true" />
		<property enableshadow="true" />
		<property enablefog="true" />
		<property skinned="false" />
	</RenderGroup>
	
	<RenderGroup id="skinned" >
		<property enablelighting="true" />
		<property enabletexture="true" />
		<property enableshadow="true" />
		<property enablefog="true" />
		<property skinned="true" />
	</RenderGroup>
	
	<RenderGroup id="sky" >
		<property enablelighting="true" />
		<property enabletexture="true" />
		<property enableshadow="false" />
		<property enablefog="true" />
		<property skinned="false" />
	</RenderGroup>
	
	<RenderGroup id="skydeferred" >
		<property enablelighting="false" />
		<property enabletexture="true" />
		<property enableshadow="false" />
		<property enablefog="true" />
		<property skinned="false" />
	</RenderGroup>

</RenderGroupResource>